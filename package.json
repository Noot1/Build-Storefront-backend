{
  "name": "nootprojecttwo",
  "version": "1.0.0",
  "description": "this project is for awarded udacity nano degrees",
  "main": "server.js",
  "scripts": {
    "test": "cross-env ENV=test npm run migrate-and-jasmine",
    "dev": "db-migrate up --env dev",
    "clean": "rimraf build/",
    "build": "npx tsc",
    "start": " nodemon src/server.ts",
    "format": "prettier --write src/**/*.ts",
    "lint": "eslint --config .eslintrc.js ./src/**/*.ts",
    "prettier": "prettier --config .prettierrc 'src/**/*.ts' --write",
    "jasmine": "jasmine; exit 0",
    "migrate-up": "set ENV=dev&& db-migrate up",
    "migrate-down": "set ENV=dev&& db-migrate down",
    "migrate-and-jasmine": "db-migrate up -e test && npm run build && npm run jasmine && db-migrate reset -e test"
  },
  "keywords": [],
  "author": "Udacity",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.0.1",
    "cors": "^2.8.5",
    "db-migrate": "^0.11.13",
    "db-migrate-pg": "^1.2.2",
    "dotenv": "^10.0.0",
    "git-install": "^0.5.2",
    "jasmine-spec-reporter": "^7.0.0",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.10.0",
    "pg": "^8.7.3",
    "pg-pool": "^3.5.1"
  },
  "devDependencies": {
    "@ert78gb/jasmine-ts": "^0.3.2",
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.13",
    "@types/jasmine": "^3.10.6",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/morgan": "^1.9.3",
    "@types/node": "^16.11.12",
    "@types/pg": "^8.6.5",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.25.0",
    "@typescript-eslint/parser": "^5.25.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.15.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "express": "^4.18.1",
    "i": "^0.3.7",
    "jasmine": "^4.1.0",
    "nodemon": "^2.0.15",
    "npm": "^8.11.0",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2",
    "supertest": "^6.2.3",
    "ts-node": "^10.8.0",
    "typescript": "^4.7.2"
  }
}
